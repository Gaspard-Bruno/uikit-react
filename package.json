{
  "name": "uikit-react",
  "version": "0.0.5",
  "description": "UIkit components built with React",
  "main": "./lib",
  "scripts": {
    "test": "jest",
    "prebuild": "rm -rf lib",
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "npm run build"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/classnames"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stipsan/uikit-react.git"
  },
  "keywords": [
    "react",
    "react-component",
    "uikit",
    "ecosystem-react"
  ],
  "author": "stipsan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stipsan/uikit-react/issues"
  },
  "homepage": "https://uikit-react.firebaseapp.com/",
  "peerDependencies": {
    "react": ">=0.12.x || >=0.13.x || >=0.14.0-alpha1"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-jest": "^5.3.0",
    "jest-cli": "^0.4.14",
    "react": "^0.13.3"
  },
  "dependencies": {
    "classnames": "^2.1.3"
  }
}
