{
  "name": "uikit-react",
  "version": "0.0.1",
  "description": "UIkit components for React",
  "main": "src/index.js",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js"],
    "moduleFileExtensions": ["js", "json", "es6"],
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/classnames"
    ]
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stipsan/react-uikit.git"
  },
  "keywords": [
    "react",
    "react-component",
    "uikit",
    "ecosystem-react"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stipsan/react-uikit/issues"
  },
  "homepage": "https://github.com/stipsan/react-uikit",
  "peerDependencies": {
    "react": ">=0.12.x || >=0.13.x || >=0.14.0-alpha1"
  },
  "devDependencies": {
    "babel-jest": "^5.3.0",
    "jest-cli": "^0.4.14",
    "react": "^0.13.3"
  },
  "dependencies": {
    "classnames": "^2.1.3"
  }
}
