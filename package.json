{
  "name": "uikit-react",
  "version": "0.0.10",
  "description": "UIkit components built with React",
  "main": "./lib",
  "scripts": {
    "test": "jest",
    "prebuild": "rm -rf lib",
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "npm run build",
    "webpack": "webpack-dev-server --config webpack.config.js --content-base build --inline",
    "dist": "NODE_ENV=production webpack --debug false --devtool false --optimize-dedupe -p",
    "postinstall": "bower install",
    "start": "babel-node docs/server"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/classnames"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stipsan/uikit-react.git"
  },
  "keywords": [
    "react",
    "react-component",
    "uikit",
    "ecosystem-react"
  ],
  "author": "Stian Didriksen <stipsan@gmail.com> (http://github.com/stipsan)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stipsan/uikit-react/issues"
  },
  "homepage": "https://uikit-react.firebaseapp.com/",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "bower": "^1.7.9",
    "brfs": "^1.4.3",
    "browser-sync": "^2.12.8",
    "browserify": "^13.0.1",
    "buildbranch": "1.0.0",
    "codemirror": "^5.14.2",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.2.0",
    "eslint": "^2.10.0",
    "eslint-config-stipsan": "1.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "express-history-spa-fallback": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-less": "^3.1.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-notify": "^2.2.0",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "include-folder": "^1.0.0",
    "less-plugin-bower-resolve": "^0.1.0",
    "less-plugin-npm-import": "^2.1.0",
    "merge-stream": "^1.0.0",
    "node-libs-browser": "^1.0.0",
    "pretty-hrtime": "^1.0.2",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "require-dir": "^0.3.0",
    "underscore": "^1.8.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  }
}
