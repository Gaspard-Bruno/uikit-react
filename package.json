{
  "name": "uikit-react",
  "version": "0.0.10",
  "description": "UIkit components built with React",
  "main": "./lib",
  "scripts": {
    "test": "jest",
    "prebuild": "rm -rf lib",
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "npm run build",
    "webpack": "webpack-dev-server --config webpack.config.js --content-base build --inline",
    "dist": "NODE_ENV=production webpack --debug false --devtool false --optimize-dedupe -p",
    "postinstall": "bower install",
    "start": "gulp"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/classnames"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stipsan/uikit-react.git"
  },
  "keywords": [
    "react",
    "react-component",
    "uikit",
    "ecosystem-react"
  ],
  "author": "stipsan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stipsan/uikit-react/issues"
  },
  "homepage": "https://uikit-react.firebaseapp.com/",
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-jest": "^5.3.0",
    "jest-cli": "^0.4.15",
    "react": "^0.14.0-beta1"
  },
  "devDependencies": {
    "babel-core": "^5.6.17",
    "babel-loader": "^5.3.1",
    "bower": "^1.4.1",
    "brfs": "^1.4.0",
    "browser-sync": "^2.7.13",
    "browserify": "^10.2.4",
    "buildbranch": "0.0.3",
    "codemirror": "^5.4.0",
    "connect-history-api-fallback": "^1.1.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.0",
    "gulp-notify": "^2.2.0",
    "gulp-replace": "^0.5.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "include-folder": "^0.9.0",
    "less-plugin-bower-resolve": "^0.1.0",
    "less-plugin-npm-import": "^2.1.0",
    "merge-stream": "^0.1.8",
    "node-libs-browser": "^0.5.2",
    "pretty-hrtime": "^1.0.0",
    "react": "^0.14.0-beta1",
    "react-dom": "^0.14.0-beta1",
    "react-router": "^1.0.0-beta3",
    "require-dir": "^0.3.0",
    "underscore": "^1.8.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.10.1"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "react-addons-transition-group": "^0.14.0-beta1"
  }
}
